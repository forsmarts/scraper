- const allOdds = data.allCoefficients.combined_odds
- const matchResult = data.allCoefficients.matchResult
- const matchTime = data.allCoefficients.matchTime
- const isLive = data.allCoefficients.isLive
- const greenRatio = 0.95

if allOdds != ''
  if matchTime != ''
    h2 Score: #{matchResult} Time: #{matchTime} 
  table(class=isLive == 'true' ? 'liveodds' : 'odds')
    tr
      th 
      each odd in allOdds
        th= odd.displayName
    tr
      td B
      each odd in allOdds
        td= odd.betfairBack
    tr
      td L
      each odd in allOdds
        td= odd.betfairLay
    tr
      td A
      each odd in allOdds
        td= odd.betfairAverage
    tr
      td I
      each odd in allOdds
        td= odd.iddaaOdd
    tr
      td R
      each odd in allOdds
        if odd.isValid & odd.ratio > greenRatio
          td.marked= odd.ratio
        else 
          td= odd.ratio
